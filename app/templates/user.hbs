<div id="subheader" class="row">
  <div class="small-11 end columns">
    <h6><span class="hot">Committen</span>Dashboard</h6>
  </div>
</div>

<div class="row">
  <div class="small-4 column">
    {{user-profile user=content}}
  </div>

  <div class="small-8 column">
    <h6>Commit History</h6>
    {{#each commit in commits}}
    <div class="small-6 columns">
      <p>{{commit.date}}</p>
      <p>{{commit.message}}</p>
      <a href="">Check me out on GitHub</a>
    </div>
    {{/each}}
  </div>
</div>

<div class="row">
  {{#each goal in goals}}
  <div class="small-6 columns">
    {{user-goal goal=goal}}
  </div>
  {{/each}}
  {{#link-to 'goal.new'}}
    Make a goal
  {{/link-to}}
  {{outlet}}
</div>
